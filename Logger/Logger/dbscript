/****** Object:  Table [dbo].[Log]    Script Date: 13.09.2018 23:40:55 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE TABLE [dbo].[Log](
	[Id] [int] IDENTITY(1,1) NOT NULL,
	[PcName] [nvarchar](256) NOT NULL,
	[UserName] [nvarchar](256) NOT NULL,
	[Type] [tinyint] NULL,
	[Time] [datetime] NULL,
PRIMARY KEY CLUSTERED 
(
	[Id] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]
) ON [PRIMARY]
GO
/****** Object:  Table [dbo].[WorkedTime]    Script Date: 13.09.2018 23:40:56 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE TABLE [dbo].[WorkedTime](
	[Id] [int] IDENTITY(1,1) NOT NULL,
	[LogId] [int] NOT NULL,
	[TimeInSeconds] [int] NULL,
PRIMARY KEY CLUSTERED 
(
	[Id] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]
) ON [PRIMARY]
GO
ALTER TABLE [dbo].[WorkedTime]  WITH CHECK ADD FOREIGN KEY([LogId])
REFERENCES [dbo].[Log] ([Id])
GO
ALTER TABLE [dbo].[WorkedTime]  WITH CHECK ADD  CONSTRAINT [check_time] CHECK  (([WorkedTime].[TimeInSeconds]>(0)))
GO
ALTER TABLE [dbo].[WorkedTime] CHECK CONSTRAINT [check_time]
GO
/****** Object:  StoredProcedure [dbo].[GetLastLoginProc]    Script Date: 13.09.2018 23:40:56 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE PROC [dbo].[GetLastLoginProc] @pcname nvarchar(256),@user_name nvarchar(256),@type tinyint,@return datetime output,@returnid int output
AS
	BEGIN

		SELECT  @return=[Time], @returnid=Id FROM dbo.Log
			WHERE PcName = @pcname AND
			UserName = @user_name 
			AND
			Type=@type
			ORDER BY Id DESC 
	END
GO
USE [master]
GO
